<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:chrome="http://www.mozilla.org/rdf/chrome#">

	<!-- list all the packages being supplied by this extension -->
	<Seq about="urn:mozilla:package:root">
		<li resource="urn:mozilla:package:libf"/>
	</Seq>

	<!-- package information -->
	<Description about="urn:mozilla:package:libf"
		chrome:author="Alex Muntean"
		chrome:authorURL="http://alex.muntean.name/"
		chrome:description="Locates a bookmark in Bookmarks Manager's folders tree or in the Bookmarks Sidebar, complementing the Bookmark Search feature of Firefox."
		chrome:displayName="Locate in Bookmark Folders"
		chrome:name="libf"
		chrome:extension="true"/>

	<!-- list all the system overlays that will be injected by this extension -->
	<Seq about="urn:mozilla:overlays">
		<li resource="chrome://browser/content/browser.xul"/>
		<li resource="chrome://browser/content/bookmarks/bookmarksPanel.xul"/>
		<li resource="chrome://browser/content/bookmarks/bookmarksManager.xul"/>
	</Seq>

	<!-- list which dynamic overlay is injected into which system overlay -->

	<Seq about="chrome://browser/content/browser.xul">
		<li>chrome://libf/content/libfOverlayBrowser.xul</li>
	</Seq>

	<Seq about="chrome://browser/content/bookmarks/bookmarksPanel.xul">
		<li>chrome://libf/content/libfOverlayBookmarksPanel.xul</li>
	</Seq>

	<Seq about="chrome://browser/content/bookmarks/bookmarksManager.xul">
		<li>chrome://libf/content/libfOverlayBookmarksManager.xul</li>
	</Seq>

</RDF>
