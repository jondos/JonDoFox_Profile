<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>Startingpage HTTPS</os:ShortName>
<os:Description>Startingpage HTTPS Search Bar</os:Description>
<os:InputEncoding>utf-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/x-icon;base64,</os:Image>
<UpdateInterval>3</UpdateInterval>
<UpdateUrl>https://startingpage.com/toolbar/searchbar/en/startingpage_ff_secure_en.src</UpdateUrl>
<IconUpdateUrl>https://startingpage.com/toolbar/searchbar/en/startingpage.png</IconUpdateUrl>
<os:Url type="text/html" method="POST" template="https://startingpage.com/do/metasearch.pl">
  <os:Param name="query" value="{searchTerms}"/>
  <os:Param name="cat" value="web"/>
  <os:Param name="pl" value="ff"/>
  <os:Param name="language" value="english"/>
</os:Url>
</SearchPlugin>