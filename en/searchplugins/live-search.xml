<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>Live Search</os:ShortName>
<os:Description>Use Live Search to search the web.</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/x-icon;base64,</os:Image>
<UpdateInterval>7</UpdateInterval>
<UpdateUrl>http://mycroft.mozdev.org/update.php/id0/livecom.src</UpdateUrl>
<IconUpdateUrl>http://mycroft.mozdev.org/update.php/id0/livecom.png</IconUpdateUrl>
<SearchForm>http://www.live.com</SearchForm>
<os:Url type="text/html" method="GET" template="http://search.live.com/results.aspx?q={searchTerms}&amp;sourceid=Mozilla-search&amp;form=CHROME">
</os:Url>
</SearchPlugin>