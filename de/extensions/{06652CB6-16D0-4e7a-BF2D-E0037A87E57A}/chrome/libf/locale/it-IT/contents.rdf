<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:chrome="http://www.mozilla.org/rdf/chrome#">

	<!-- list all the packages being supplied by this extension -->
	<Seq about="urn:mozilla:locale:root">
		<li resource="urn:mozilla:locale:it-IT"/>
	</Seq>

	<!-- locale information -->
	<Description about="urn:mozilla:locale:it-IT"
		chrome:author="Luca Pedrazzi"
		chrome:name="it-IT"
		chrome:displayName="Italiano">
		<chrome:packages>
			<Seq about="urn:mozilla:locale:it-IT:packages">
				<li resource="urn:mozilla:locale:it-IT:libf"/>
			</Seq>
		</chrome:packages>
	</Description>

</RDF>
